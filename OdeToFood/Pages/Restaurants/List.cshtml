@page
@model ListModel
@{}

<h1>Restaurants</h1>

<table class="table thead-dark">
    <tr>
        <th>Name</th>
        <th>Location</th>
        <th>Cusine type</th>
    </tr>

    @foreach (var restaurant in Model.Restaurants)
    {
        <tr>
            <td>@restaurant.Name</td>
            <td>@restaurant.Location</td>
            <td>@restaurant.Cusine</td>
        </tr>
    }

</table>